{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"name": "Primary", 
					"slug": "primary",
					"color": "#FA008A"
				},
				{
					"name": "Secondary", 
					"slug": "secondary",
					"color": "#00E5E5"
				},
				{
					"name": "Accent Magenta", 
					"slug": "accent-magenta",
					"color": "#C3006A"
				},
				{
					"name": "Accent Peach",
					"slug": "accent-peach",
					"color": "#FFC3B9"
				},
				{
					"name": "Accent Purple",
					"slug": "accent-purple",
					"color": "#9A00FA"
				},
				{
					"name": "Dark", 
					"slug": "dark",
					"color": "#0d0f05"
				},
				{
					"name": "Light",  
					"slug": "light",
					"color": "#ffffff"
				},
				{
					"name": "Alt", 
					"slug": "alt",
					"color": "#f1f2f0"
				},
				{
					"name": "Focus", 
					"slug": "focus",
					"color": "#FFDD00"
				}
			]
		},
		"layout": {
			"contentSize": "var(--wp--custom--width--container)"
		},
		"spacing": {
			"defaultFontSizes": false, 
			"defaultSpacingSizes": false,
			"spacingScale": { "steps": 0 },
			"spacingSizes": [
				{
					"name": "XS", 
					"size": "var(--wp--custom--spacing--xs)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "var(--wp--custom--spacing--sm)",
					"slug": "small"
				},
				{
					"name": "Medium", 
					"size": "var(--wp--custom--spacing--md)",
					"slug": "medium"
				},
				{
					"name": "Large", 
					"size": "var(--wp--custom--spacing--lg)",
					"slug": "large"
				},
				{
					"name": "XL", 
					"size": "var(--wp--custom--spacing--xl)",
					"slug": "x-large"
				}
			],
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": false,
			"textTransform": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "system-ui, sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Söhne Dreiviertelfett\", sans-serif",
					"name": "Söhne Dreiviertelfett",
					"slug": "primary", 
					"fontFace": [
						{
							"fontFamily": "Söhne Dreiviertelfett",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontDisplay": "fallback",
							"src": [ "file:./dist/fonts/soehne/soehne-dreiviertelfett.woff2" ]
						}
					]
				},
				{
					"fontFamily": "\"Söhne Halbfett\", sans-serif",
					"name": "Söhne Halbfett",
					"slug": "secondary", 
					"fontFace": [
						{
							"fontFamily": "Söhne Halbfett",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontDisplay": "fallback",
							"src": [ "file:./dist/fonts/soehne/soehne-halbfett.woff2" ]
						}
					]
				}, 
				{
					"fontFamily": "\"Söhne Kraftig\", sans-serif",
					"name": "Söhne Kraftig",
					"slug": "tertertiary", 
					"fontFace": [
						{
							"fontFamily": "Söhne Kraftig",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontDisplay": "fallback",
							"src": [ "file:./dist/fonts/soehne/soehne-kraftig.woff2" ]
						}
					]
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "0.875rem",
					"fluid": false, 
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": "1rem",
					"fluid": false, 
					"slug": "sm"
				},
				{
					"name": "Body",
					"size": "1.0625rem",
					"fluid": {
						"min": "1.0625rem",
						"max": "1.25rem"
					},
					"slug": "body"
				},
				{
					"name": "Medium",
					"size": "1.5625rem",
					"fluid": {
						"min": "1.275rem",
						"max": "1.5625rem"
					},
					"slug": "md"
				},
				{
					"name": "Large",
					"size": "1.9531rem",
					"fluid": {
						"min": "1.53rem",
						"max": "1.9531rem"
					},
					"slug": "lg"
				},
				{
					"name": "Extra Large",
					"size": "2.4414rem",
					"fluid": {
						"min": "1.836rem",
						"max": "2.4414rem"
					},
					"slug": "xl"
				},
				{
					"name": "Huge",
					"size": "3.0518rem",
					"fluid": {
						"min": "2.2032rem",
						"max": "3.0518rem"
					},
					"slug": "2xl"
				},
				{
					"name": "Massive",
					"size": "3.8147rem",
					"fluid": {
						"min": "2.6438rem",
						"max": "3.8147rem"
					},
					"slug": "3xl"
				}
			]
		},
		"useRootPaddingAwareAlignments": true, 
		"custom": {
			"spacing": {
					"gap": "1.25rem", 
					"3xs": "0.3125rem",
					"2xs": "0.625rem",
					"xs": "clamp(0.8125rem, 0.7786rem + 0.1695vi, 0.9375rem)",
					"sm": "clamp(1.0625rem, 1.0117rem + 0.2542vi, 1.25rem)",
					"md": "clamp(1.625rem, 1.5572rem + 0.339vi, 1.875rem)",
					"lg": "clamp(2.125rem, 2.0233rem + 0.5085vi, 2.5rem)",
					"xl": "clamp(3.1875rem, 3.035rem + 0.7627vi, 3.75rem)",
					"2xl": "clamp(4.25rem, 4.0466rem + 1.0169vi, 5rem)",
					"xs-s": "clamp(0.8125rem, 0.6939rem + 0.5932vi, 1.25rem)",
					"s-xxl": "clamp(1.0625rem, -0.0053rem + 5.339vi, 5rem)"
			},
			"typography": {
				"letterSpacing": {
					"body": "-0.01em",
					"heading": "-0.02em",
					"large": "0.01em",
					"uppercase": "0"
				},
				"lineHeight": {
					"body": "1.5",
					"input": "1.25",
					"heading": {
						"sm": 1.25,
						"lg": 1.05
					}
				}
			}, 
			"width": {
				"container": "70rem",
				"content": "52rem",
				"content-small": "38rem",
				"character": "68ch"
			}, 
			"height": {
				"input": "2.5rem"
			},
			"color": {
				"background": "light-dark(var(--wp--preset--color--alt), var(--wp--preset--color--dark))",
				"text": "light-dark(var(--wp--preset--color--dark), var(--wp--preset--color--light))",
				"border": "#666666",
				"success": "#457c39",
				"error": "#db2f27",
				"disabled": "#999999"
			}
		}
	},
	"styles": {
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "1em"
				}
			}, 
			"core/code": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--dark)",
					"text": "var(--wp--preset--color--light)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--sm)",
						"right": "var(--wp--custom--spacing--sm)",
						"bottom": "var(--wp--custom--spacing--sm)",
						"left": "var(--wp--custom--spacing--sm)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			}
		},
		"color": {
			"background": "var(--wp--custom--color--background)",
			"text": "var(--wp--custom--color--text)"
		},
		"elements": {
			"button": {
				"border": {
					"color": "var(--wp--custom--color--text)",
					"radius": "0",
					"style": "solid",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--custom--color--background)",
					"text": "var(--wp--custom--color--text)"
				},
				"spacing": {
					"padding": {
						"top": "1rem",
						"right": "1.6rem",
						"bottom": "1rem",
						"left": "1.6rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)",
					"lineHeight": "1",
					"fontWeight": "bold"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--3-xl)", 
					"fontWeight": "800",
					"lineHeight": "var(--wp--custom--typography--line-height--heading--lg)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--lg)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontSize": "var(--wp--preset--font-size--2-xl)", 
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--heading--lg)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--lg)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontSize": "var(--wp--preset--font-size--xl)", 
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--heading--lg)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tertiary)",
					"fontSize": "var(--wp--preset--font-size--lg)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--body)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tertiary)",
					"fontSize": "var(--wp--preset--font-size--md)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--body)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tertiary)",
					"fontSize": "var(--wp--preset--font-size--md)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--body)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--text)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--buffer, var(--wp--custom--spacing--gap))"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--body)", 
			"fontWeight": "400",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--body)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		}
	}
}