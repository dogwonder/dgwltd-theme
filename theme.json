{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"name": "Primary", 
					"slug": "primary",
					"color": "#0095a3"
				},
				{
					"name": "Secondary", 
					"slug": "secondary",
					"color": "#fe9f97"
				},
				{
					"name": "Tertiary", 
					"slug": "tertiary",
					"color": "#fbae17"
				},
				{
					"name": "Dark", 
					"slug": "dark",
					"color": "#0d0f05"
				},
				{
					"name": "Light",  
					"slug": "light",
					"color": "#ffffff"
				},
				{
					"name": "Alt", 
					"slug": "alt",
					"color": "#f1f2f0"
				},
				{
					"name": "Focus", 
					"slug": "focus",
					"color": "#FFDD00"
				}
			]
		},
		"layout": {
			"contentSize": "1120px"
		},
		"spacing": {
			"defaultFontSizes": false, 
			"defaultSpacingSizes": false,
			"spacingScale": { "steps": 0 },
			"spacingSizes": [
				{
					"name": "XS", 
					"size": "var(--wp--custom--spacing--xs)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "var(--wp--custom--spacing--sm)",
					"slug": "small"
				},
				{
					"name": "Medium", 
					"size": "var(--wp--custom--spacing--md)",
					"slug": "medium"
				},
				{
					"name": "Large", 
					"size": "var(--wp--custom--spacing--lg)",
					"slug": "large"
				},
				{
					"name": "XL", 
					"size": "var(--wp--custom--spacing--xl)",
					"slug": "x-large"
				}
			],
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": false,
			"textTransform": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Söhne Halbfett\", sans-serif",
					"name": "Söhne Halbfett",
					"slug": "primary", 
					"fontFace": [
						{
							"fontFamily": "Söhne Halbfett",
							"fontWeight": "600",
							"fontStyle": "normal",
							"src": [ "file:./dist/fonts/soehne/soehne-halbfett.woff2" ]
						}
					]
				}, 
				{
					"fontFamily": "\"Söhne Kraftig\", sans-serif",
					"name": "Söhne Kraftig",
					"slug": "secondary", 
					"fontFace": [
						{
							"fontFamily": "Söhne Kraftig",
							"fontWeight": "600",
							"fontStyle": "normal",
							"src": [ "file:./dist/fonts/soehne/soehne-kraftig.woff2" ]
						}
					]
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "0.875rem",
					"fluid": false, 
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": "1rem",
					"fluid": false, 
					"slug": "sm"
				},
				{
					"name": "Body",
					"size": "1.125rem",
					"fluid": false, 
					"slug": "body"
				},
				{
					"name": "Medium",
					"size": "1.333rem",
					"fluid": {
						"min": "1.125rem",
						"max": "1.333rem"
					},
					"slug": "md"
				},
				{
					"name": "Large",
					"size": "1.777rem",
					"fluid": {
						"min": "1.333rem",
						"max": "1.777rem"
					},
					"slug": "lg"
				},
				{
					"name": "Extra Large",
					"size": "2.369rem",
					"fluid": {
						"min": "1.333rem",
						"max": "2.369rem"
					},
					"slug": "xl"
				},
				{
					"name": "Huge",
					"size": "3.157rem",
					"fluid": {
						"min": "1.777rem",
						"max": "3.157rem"
					},
					"slug": "2xl"
				},
				{
					"name": "Massive",
					"size": "4.209rem",
					"fluid": {
						"min": "2.369rem",
						"max": "4.209rem"
					},
					"slug": "3xl"
				}
			]
		},
		"useRootPaddingAwareAlignments": true, 
		"custom": {
			"spacing": {
					"baseline": "16px",
					"gap": "1.5rem", 
					"2xs": "0.75rem",
					"xs": "clamp(0.75rem, calc(0.69rem + 0.28vw), 0.94rem)",
					"sm": "clamp(0.75rem, calc(0.60rem + 0.74vw), 1.25rem)",
					"md": "clamp(1.00rem, calc(0.74rem + 1.30vw), 1.88rem)",
					"lg": "clamp(1.50rem, calc(1.20rem + 1.48vw), 2.50rem)",
					"xl": "clamp(2.00rem, calc(1.48rem + 2.59vw), 3.75rem)",
					"2xl": "clamp(3.00rem, calc(2.41rem + 2.96vw), 5.00rem)",
					"xs-s": "clamp(0.75rem, calc(0.60rem + 0.74vw), 1.25rem)",
					"s-m": "clamp(1.00rem, calc(0.74rem + 1.30vw), 1.88rem)",
					"m-l": "clamp(1.50rem, calc(1.20rem + 1.48vw), 2.50rem)",
					"l-xl": "clamp(2.00rem, calc(1.48rem + 2.59vw), 3.75rem)",
					"xl-2xl": "clamp(3.00rem, calc(2.41rem + 2.96vw), 5.00rem)",
					"2xl-3xl": "clamp(4.00rem, calc(3.33rem + 3.33vw), 6.25rem)"
				},
				"typography": {
					"letterSpacing": {
						"body": "-0.01em",
						"heading": "-0.02em",
						"large": "0.01em",
						"uppercase": "0"
					},
					"lineHeight": {
						"body": "1.5",
						"heading": {
							"default": "1.15",
							"sm": 1.25,
							"md": 1.15,
							"xl": 1.05
						}
					}
				}, 
				"width": {
					"container": "70rem",
					"content": "52rem",
					"contentSmall": "38rem",
					"character": "68ch"
				}, 
				"color": {
					"border": "#666666",
					"success": "#457c39",
					"error": "#db2f27",
					"disabled": "#999999"
				}
		}
	},
	"styles": {
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "1em"
				}
			}, 
			"core/code": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--dark)",
					"text": "var(--wp--preset--color--light)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--sm)",
						"right": "var(--wp--custom--spacing--sm)",
						"bottom": "var(--wp--custom--spacing--sm)",
						"left": "var(--wp--custom--spacing--sm)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--alt)",
			"text": "var(--wp--preset--color--dark)"
		},
		"elements": {
			"button": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"radius": "0",
					"style": "solid",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--dark)"
				},
				"spacing": {
					"padding": {
						"top": "1rem",
						"right": "1.6rem",
						"bottom": "1rem",
						"left": "1.6rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)",
					"lineHeight": "1",
					"fontWeight": "bold"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--3-xl)", 
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--heading--lg)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--lg)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--2-xl)", 
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--heading--lg)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--lg)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontSize": "var(--wp--preset--font-size--xl)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--heading--lg)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontSize": "var(--wp--preset--font-size--lg)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--body)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontSize": "var(--wp--preset--font-size--md)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--body)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontSize": "var(--wp--preset--font-size--md)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--body)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--dark)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--buffer, var(--wp--custom--spacing--gap))"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--body)", 
			"fontWeight": "400",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--body)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		}
	}
}