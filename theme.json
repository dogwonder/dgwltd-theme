{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"name": "Primary", 
					"slug": "primary",
					"color": "#0095a3"
				},
				{
					"name": "Secondary", 
					"slug": "secondary",
					"color": "#fe9f97"
				},
				{
					"name": "Tertiary", 
					"slug": "tertiary",
					"color": "#fbae17"
				},
				{
					"name": "Dark", 
					"slug": "dark",
					"color": "#0d0f05"
				},
				{
					"name": "Light",  
					"slug": "light",
					"color": "#f1f2f0"
				},
				{
					"name": "Alt", 
					"slug": "alt",
					"color": "#e0e5db"
				}
			]
		},
		"layout": {
			"contentSize": "1120px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "var(--wp--custom--spacing--xs)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "var(--wp--custom--spacing--sm)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "var(--wp--custom--spacing--md)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "var(--wp--custom--spacing--lg)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "var(--wp--custom--spacing--xl)",
					"slug": "70",
					"name": "5"
				}
			],
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": false,
			"textTransform": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Söhne Halbfett\", sans-serif",
					"name": "Söhne Halbfett",
					"slug": "soehne-halbfett", 
					"fontFace": [
						{
							"fontFamily": "Söhne Halbfett",
							"fontWeight": "600",
							"fontStyle": "normal",
							"src": [ "file:./dist/fonts/soehne/soehne-halbfett.woff2" ]
						}
					]
				}, 
				{
					"fontFamily": "\"Söhne Kraftig\", sans-serif",
					"name": "Söhne Kraftig",
					"slug": "soehne-kraftig", 
					"fontFace": [
						{
							"fontFamily": "Söhne Kraftig",
							"fontWeight": "600",
							"fontStyle": "normal",
							"src": [ "file:./dist/fonts/soehne/soehne-kraftig.woff2" ]
						}
					]
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "0.875rem",
					"fluid": false, 
					"slug": "tiny"
				},
				{
					"name": "Small",
					"size": "1rem",
					"fluid": false, 
					"slug": "small"
				},
				{
					"name": "Body",
					"size": "1.125rem",
					"fluid": false, 
					"slug": "body"
				},
				{
					"name": "Medium",
					"size": "1.333rem",
					"fluid": {
						"min": "1.125rem",
						"max": "1.333rem"
					},
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.777rem",
					"fluid": {
						"min": "1.333rem",
						"max": "1.777rem"
					},
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "2.369rem",
					"fluid": {
						"min": "1.333rem",
						"max": "2.369rem"
					},
					"slug": "extra-large"
				},
				{
					"name": "Huge",
					"size": "3.157rem",
					"fluid": {
						"min": "1.777rem",
						"max": "3.157rem"
					},
					"slug": "huge"
				},
				{
					"name": "Massive",
					"size": "4.209rem",
					"fluid": {
						"min": "2.369rem",
						"max": "4.209rem"
					},
					"slug": "massive"
				}
			]
		},
		"useRootPaddingAwareAlignments": true, 
		"custom": {
			"spacing": {
				"baseline": "16px",
				"2xs": "0.75rem",
				"xs": "clamp(0.75rem, calc(0.69rem + 0.28vw), 0.94rem)",
				"sm": "clamp(0.75rem, calc(0.60rem + 0.74vw), 1.25rem)",
				"md": "clamp(1.00rem, calc(0.74rem + 1.30vw), 1.88rem)",
				"lg": "clamp(1.50rem, calc(1.20rem + 1.48vw), 2.50rem)",
				"xl": "clamp(2.00rem, calc(1.48rem + 2.59vw), 3.75rem)",
				"gap": "1.5rem"
			},
			"typography": {
				"letter-spacing": {
					"body": "-0.01em",
					"heading": "-0.02em",
					"gigantic": "0.01em",
					"uppercase": "0"
				},
				"line-height": {
					"body": "1.5",
					"headings": {
						"gigantic": 1.05,
						"large": 1.15,
						"small": 1.25
					}
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "1em"
				}
			}, 
			"core/code": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--dark)",
					"text": "var(--wp--preset--color--light)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--sm)",
						"right": "var(--wp--custom--spacing--sm)",
						"bottom": "var(--wp--custom--spacing--sm)",
						"left": "var(--wp--custom--spacing--sm)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--light)",
			"text": "var(--wp--preset--color--dark)"
		},
		"elements": {
			"button": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"radius": "0",
					"style": "solid",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--dark)"
				},
				"spacing": {
					"padding": {
						"top": "1rem",
						"right": "1.6rem",
						"bottom": "1rem",
						"left": "1.6rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)",
					"lineHeight": "1",
					"fontWeight": "bold"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--soehne-halbfett)",
					"fontSize": "var(--wp--preset--font-size--massive)", 
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--gigantic)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--gigantic)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--soehne-halbfett)",
					"fontSize": "var(--wp--preset--font-size--huge)", 
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--gigantic)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--gigantic)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--soehne-kraftig)",
					"fontSize": "var(--wp--preset--font-size--extra-large)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--large)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--soehne-kraftig)",
					"fontSize": "var(--wp--preset--font-size--large)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--body)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--soehne-kraftig)",
					"fontSize": "var(--wp--preset--font-size--medium)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--body)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--soehne-kraftig)",
					"fontSize": "var(--wp--preset--font-size--medium)", 
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--body)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--dark)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--buffer, var(--wp--custom--spacing--gap))"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--body)", 
			"fontWeight": "400",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--body)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		}
	}
}