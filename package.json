{
  "name": "dgwltd",
  "version": "1.2.222",
  "type": "module",
  "main": "",
  "scripts": {
    "clean": "trash \"./dist/\"",
    "css:sass:main": "sass --silence-deprecation=mixed-decls --quiet-deps ./src/assets/scss/main.scss ./dist/css/main.css",
    "css:sass:print": "sass ./src/assets/scss/print.scss ./dist/css/print.css",
    "css:sass:wp": "sass ./src/assets/scss/wp.scss ./dist/css/wp.css",
    "css:sass": "run-s css:sass:main css:sass:print css:sass:wp",
    "css:replace": "replace \"Version: v\\d+(\\.\\d+){1,2}\" \"Version: $(npm version patch --no-git-tag-version)\" style.css",
    "css:postcss": "postcss ./dist/css/**/*.css --use autoprefixer postcss-custom-properties cssnano --no-map --verbose --dir ./dist/css/",
    "css": "run-s css:*",
    "scripts:concat": "concat ./src/assets/scripts/app.js -o ./src/assets/scripts/app.min.js",
    "scripts:minify": "uglifyjs ./src/assets/scripts/app.min.js --compress --output ./src/assets/scripts/app.min.js",
    "scripts": "run-s scripts:*",
    "watch:css": "onchange \"./src/assets/**/*.scss\" -- npm run css:sass",
    "watch:eleventy": "npx eleventy --serve",
    "watch:scripts": "onchange \"./src/assets/**/*.js\" -- npm run scripts:concat",
    "watch": "run-p watch:*",
    "tests:dev": "RUN_LOCALLY=true cypress run",
    "tests:prod": "cypress run",
    "cypress:run:sw": "RUN_LOCALLY=true cypress run --spec 'cypress/e2e/sw.cy.js'",
    "cms": "npx tinacms dev -c \"npm run watch\"",
    "favicon": "node ./scripts/favicon.js",
    "inquirer": "node ./scripts/inquirer.mjs",
    "build": "NODE_ENV=production npm run clean && NODE_ENV=production npm run-s css scripts && NODE_ENV=production npx eleventy && NODE_ENV=production npm run favicon"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^5.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^3.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/import": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "concat": "^1.0.3",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "cssnano": "^6.0.1",
    "dayjs": "^1.11.11",
    "govuk-frontend": "^5.7.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.32",
    "postcss-cli": "^10.1.0",
    "postcss-custom-properties": "^13.3.2",
    "postcss-import": "^15.1.0",
    "postcss-scss": "^4.0.9",
    "replace": "^1.2.2",
    "sass": "^1.77.3",
    "sharp": "^0.33.3",
    "svg-to-ico": "^1.0.14",
    "trash-cli": "^5.0.0",
    "typescript": "5.5.2",
    "uglify-js": "^3.17.4"
  },
  "devDependencies": {
    "axe-core": "^4.5.2",
    "@cypress-audit/lighthouse": "^1.4.2",
    "@tinacms/cli": "^1.6.9",
    "@types/node": "^22.7.6",
    "cypress": "^13.15.1",
    "cypress-audit": "^1.1.0",
    "cypress-axe": "^1.5.0",
    "cypress-puppeteer-plugin": "^1.0.1",
    "inquirer": "^10.1.8",
    "mochawesome": "^7.1.3",
    "puppeteer": "^23.6.1",
    "tinacms": "^2.2.9"
  }
}
