{
  "name": "dgwltd",
  "version": "1.2.5",
  "description": "",
  "main": "",
  "scripts": {
    "clean": "trash \"./dist/\"",
    "css:sass": "sass ./src/scss/main.scss ./dist/css/main.css  && sass ./src/scss/print.scss ./dist/css/print.css",
    "css:postcss": "postcss ./dist/css/**/*.css --use autoprefixer postcss-custom-properties cssnano --no-map --verbose --dir ./dist/css/",
    "css": "run-s css:*",
    "scripts:concat": "concat ./src/scripts/app.js -o ./src/scripts/app.min.js",
    "scripts:minify": "uglifyjs ./src/scripts/app.min.js --compress --output ./src/scripts/app.min.js",
    "scripts": "run-s scripts:*",
    "watch:css": "onchange \"./src/**/*.scss\" -- npm run css:sass",
    "watch:eleventy": "npx eleventy --serve",
    "watch": "run-p watch:*",
    "build": "NODE_ENV=production npm run clean && NODE_ENV=production npm run-s css scripts && NODE_ENV=production npx eleventy",
    "tests": "cypress run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "axe-core": "^4.5.2",
    "bootstrap": "^5.2.2",
    "concat": "^1.0.3",
    "concurrently": "^7.5.0",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.14",
    "cypress": "^12.3.0",
    "cypress-axe": "^1.0.0",
    "govuk-frontend": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0",
    "postcss-custom-properties": "^13.0.0",
    "postcss-import": "^15.0.0",
    "postcss-scss": "^4.0.5",
    "sass": "^1.56.1",
    "trash-cli": "^5.0.0",
    "uglify-js": "^3.17.4"
  }
}
